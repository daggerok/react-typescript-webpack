{
  "name": "react-typescript-webpack",
  "version": "1.0.0",
  "description": "react-typescript-webpack [![build](https://travis-ci.org/daggerok/react-typescript-webpack.svg?branch=master)](https://travis-ci.org/daggerok/react-typescript-webpack) ========================",
  "main": "./src/index.tsx",
  "scripts": {
    "start": "npm-run-all wp:compile start-in-parallel",
    "start-in-parallel": "npm-run-all --parallel wp:watch bs",
    "bs": "browser-sync start --config=./config/browser-sync.js",
    "wp:watch": "webpack --config ./config/webpack.js -w",
    "wp:compile": "webpack --config ./config/webpack.js",
    "tsc:watch": "tsc -w -p ./src",
    "tsc:compile": "tsc -p ./src",
    "postinstall": "npm-run-all typings:*",
    "typings:rm": "rimraf typings*",
    "typings:init": "typings init ./src",
    "typings:react": "typings install --global --save dt~react dt~react-dom dt~jquery dt~bootstrap"
  },
  "author": "Maksim Kostromin",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.13.0",
    "http-proxy-middleware": "^0.15.2",
    "npm-run-all": "^2.1.1",
    "rimraf": "^2.5.2",
    "source-map-loader": "^0.1.5",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "bootstrap": "^3.3.6",
    "jquery": "^2.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/react-typescript-webpack.git"
  },
  "keywords": [
    "react",
    "typescript",
    "webpack"
  ],
  "bugs": {
    "url": "https://github.com/daggerok/react-typescript-webpack/issues"
  },
  "homepage": "https://github.com/daggerok/react-typescript-webpack#readme"
}
